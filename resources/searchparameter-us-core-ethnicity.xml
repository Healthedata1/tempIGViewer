<?xml version="1.0" encoding="UTF-8"?>
<SearchParameter xmlns="http://hl7.org/fhir">
	<id value="us-core-ethnicity"/>
	<url value="http://hl7.org/fhir/us/core/SearchParameter/us-core-ethnicity"/>
	<name value="USCoreEthnicity"/>
	<status value="draft"/>
	
	<date value="2016-12-22"/>
	<publisher value="US Realm Steering Committee"/>
	<contact>
		<telecom>
			<system value="other"/>
			<value value="http://www.healthit.gov/"/>
		</telecom>
	</contact>
	<code value="ethnicity"/>
	<base value="Patient"/>
	<type value="token"/>
	<description value="Returns patients with an ethnicity extension matching the specified code."/>
	<expression value="Patient.extension.where(url = 'http://hl7.org/fhir/us/core/StructureDefinition/us-core-ethnicity').extension.value.code"></expression>
	<xpath value="f:Patient/f:extension[@url='http://hl7.org/fhir/us/core/StructureDefinition/us-core-ethnicity']/f:extension/f:valueCoding/f:code/@value"/>
	<xpathUsage value="normal"/>
</SearchParameter>
